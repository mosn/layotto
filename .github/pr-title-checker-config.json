{
  "LABEL": {
    "name": "title needs formatting",
    "color": "EEEEEE"
  },
  "CHECKS": {
    "prefixes": [
      "fix: ",
      "feat: ",
      "doc: ",
      "ci: ",
      "chore: ",
      "[WIP]"
    ],
    "regexp": "docs\\(v[0-9]\\): ",
    "regexpFlags": "i",
    "ignoreLabels": [
      "dont-check-PRs-with-this-label",
      "meta"
    ]
  },
  "MESSAGES": {
    "success": "All OK",
    "failure": "Failing CI PR title check",
    "notice": ""
  }
}